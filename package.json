{
  "name": "intro-to-gql",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "run-p watch:server start:client",
    "start:client": "react-scripts start",
    "start:server": "node dist/server.js",
    "watch:server": "nodemon --exec npm run restart",
    "build": "react-scripts build",
    "build:babel": "babel server --out-dir dist",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "clean": "rimraf build dist",
    "restart": "rimraf dist && npm run build:babel && npm run start:server"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "express": "^4.16.4",
    "graphql-codegen-core": "^0.14.5",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.5",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0",
    "typescript": "^3.3.3"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test-db-setup.js",
    "testPathIgnorePatterns": [
      "dist/"
    ],
    "restoreMocks": true
  },
  "dependencies": {
    "adal-node": "^0.1.28",
    "apollo-boost": "^0.1.28",
    "apollo-link-context": "^1.0.14",
    "apollo-server": "^2.2.5",
    "azure-keyvault": "^3.0.4",
    "bcrypt": "^3.0.2",
    "cors": "^2.8.5",
    "cuid": "^2.1.4",
    "dotenv": "^6.2.0",
    "formik": "^1.5.1",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.3",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.14",
    "prisma": "^1.26.6",
    "react": "^16.8.2",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.2",
    "validator": "^10.11.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
